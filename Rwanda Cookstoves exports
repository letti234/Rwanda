//Subset data
var quality_2000 = data.filter(ee.Filter.date('2000-01-01', '2000-12-31'))
.select("Quality").toBands();

var count_2000 = quality_2000.bitCount();

Export.image.toDrive({
  image: count_2000,
  scale: 250,
  description: '2000_QualCount',
  fileFormat: 'GEOTIFF',
  folder: "Cookstove_exports",
  maxPixels: 9e10,
  region: studyRegion
});


